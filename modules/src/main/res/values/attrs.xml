<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DatePicker">
        <attr name="dp_title" format="string|reference" />
        <attr name="dp_titleColor" format="integer|reference"/>
        <attr name="dp_hint" format="string|reference" />
        <attr name="dp_border" format="integer|reference"/>
        <attr name="dp_background" format="integer|reference"/>
    </declare-styleable>

    <declare-styleable name="Spinner">
        <attr name="sp_title" format="string|reference" />
        <attr name="sp_titleColor" format="integer|reference"/>
        <attr name="sp_hint" format="string|reference" />
        <attr name="sp_border" format="integer|reference"/>
        <attr name="sp_background" format="integer|reference"/>
</declare-styleable>

    <declare-styleable name="Button">
        <attr name="btn_title" format="string|reference"/>
        <attr name="btn_border" format="integer|reference"/>
        <attr name="btn_srcCompat" format="integer|reference"/>
        <attr name="btn_textColor" format="integer|reference"/>
        <attr name="btn_textSize" format="dimension|reference"/>
        <attr name="btn_icon_direction" format="enum">
            <enum name="RIGHT" value="0"/>
            <enum name="LEFT" value="1"/>
        </attr>
    </declare-styleable>
</resources>